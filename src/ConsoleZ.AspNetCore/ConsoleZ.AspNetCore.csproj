<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.2</TargetFramework>

    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Guy Langston</Authors>
    <PackageProjectUrl>https://github.com/guylangston/ConsoleZ</PackageProjectUrl>
    <RepositoryUrl>https://github.com/guylangston/ConsoleZ</RepositoryUrl>
    <AssemblyVersion>0.1.0.0</AssemblyVersion>
    <FileVersion>0.1.0.0</FileVersion>
    <Description>ASP.NET Core Controller for web-console and rendering</Description>
    <Copyright>MIT</Copyright>
    <Version>0.1.3</Version>
    <PackageLicenseUrl>https://raw.githubusercontent.com/guylangston/ConsoleZ/master/LICENSE</PackageLicenseUrl>
    <PackageReleaseNotes>Still-In-Development / Experimental.</PackageReleaseNotes>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

 

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.2.2" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Formatters.Json" Version="2.2.0" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.ViewFeatures" Version="2.2.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\ConsoleZ\ConsoleZ.csproj" />
  </ItemGroup>

  

</Project>
